{
  "collectionName": "per_5s",
  "info": {
    "displayName": "for every 5 models",
    "description": ""
  },
  "options": {},
  "attributes": {
    "model": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::model.model"
    },
    "wargear_to_replace": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wargear.wargear"
    },
    "weapons_to_replace": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::weapon.weapon"
    },
    "max_models_that_can_do_this": {
      "type": "integer",
      "min": 1,
      "required": true
    },
    "can_select_duplicates": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "options": {
      "type": "component",
      "repeatable": true,
      "component": "datasheet.weapon-option-replacement"
    }
  }
}
